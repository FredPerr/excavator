{
  "$schema": "https://json.schemastore.org/package",
  "private": false,
  "name": "@fredperr/excavator-ui",
  "version": "0.2.3",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "keywords": [
    "payloadcms",
    "plugin",
    "excavator",
    "utility",
    "website",
    "components",
    "ui"
  ],
  "files": [
    "dist/**"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./primitives": {
      "import": "./dist/primitives/index.mjs",
      "types": "./dist/primitives/index.d.ts",
      "require": "./dist/primitives/index.js"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "types": "./dist/components/index.d.ts",
      "require": "./dist/components/index.js"
    },
    "./utils": {
      "import": "./dist/utils/index.mjs",
      "types": "./dist/utils/index.d.ts",
      "require": "./dist/utils/index.js"
    }
  },
  "scripts": {
    "dev": "tsup src --format esm,cjs --watch --dts --external react",
    "build": "tsup src --format esm,cjs --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest run"
  },
  "peerDependencies": {
    "@payloadcms/ui": "catalog:",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@tailwindcss/typography": "catalog:",
    "class-variance-authority": "catalog:",
    "clsx": "catalog:",
    "lucide-react": "catalog:",
    "tailwind-merge": "catalog:",
    "tailwindcss": "catalog:",
    "tailwindcss-animate": "catalog:"
  },
  "devDependencies": {
    "@fredperr/excavator-config-eslint": "workspace:*",
    "@fredperr/excavator-config-ts": "workspace:*",
    "@testing-library/dom": "catalog:",
    "@testing-library/jest-dom": "catalog:",
    "@testing-library/react": "catalog:",
    "@testing-library/user-event": "catalog:",
    "global-jsdom": "catalog:",
    "@vitejs/plugin-react": "catalog:",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1",
    "eslint": "catalog:",
    "postcss": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@fredperr/excavator": "workspace:*",
    "@fredperr/excavator-config-tailwind": "workspace:*",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
