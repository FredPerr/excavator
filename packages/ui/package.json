{
  "$schema": "https://json.schemastore.org/package",
  "private": false,
  "name": "@fredperr/excavator-ui",
  "version": "0.2.0",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "license": "MIT",
  "files": [
    "dist/**"
  ],
  "exports": {
    "./button": "./src/Button.tsx",
    ".": "./dist/index.js"
  },
  "scripts": {
    "build:tw": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "dev": "tsup src/index.tsx --format esm,cjs --watch --dts --external react",
    "build": "tsup src/index.tsx --format esm,cjs --dts --external react",
    "lint": "eslint \"src/**/*.ts*\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@fredperr/excavator-config-ts": "workspace:*",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "eslint": "catalog:",
    "@fredperr/excavator-config-eslint": "workspace:*",
    "tsup": "catalog:",
    "postcss": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "react": "catalog:",
    "react-dom": "catalog:",
    "@fredperr/excavator-config-tailwind": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-dom": "npm:types-react-dom@19.0.0-rc.1"
  }
}
